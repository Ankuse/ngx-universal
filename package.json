{
  "name": "ngx-universal",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "ng build && yarn copyStaticFiles",
    "copyStaticFiles": "cp ./README.md ./LICENSE ./dist",
    "release:lib": "npm run prepublish:lib && cd ./lib && npm i && npm run standard",
    "prepublish:lib": "npm run build",
    "publish:lib": "npm publish ./dist --readme --access public",
    "release:version": "npm run release:lib && npm run publish:lib",
    "format:check": "prettier --write --config ./.prettierrc --list-different \"lib/src/*{.ts,.json,.scss}\""
  },
  "private": false,
  "dependencies": {
    "@angular/common": "8.2.4",
    "@angular/compiler-cli": "8.2.4",
    "@angular/compiler": "8.2.4",
    "@angular/core": "8.2.4",
    "@angular/platform-browser": "8.2.4",
    "@nguniversal/express-engine": "8.1.1",
    "rxjs": "6.5.2",
    "typescript": "3.5.1",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.2",
    "@angular-devkit/build-ng-packagr": "0.803.2",
    "@angular/platform-browser-dynamic": "8.2.4",
    "@angular/cli": "8.3.2",
    "@angular/compiler-cli": "8.2.4",
    "@angular/language-service": "8.2.4",
    "@types/jasmine": "3.4.0",
    "codelyzer": "5.1.0",
    "ng-packagr": "5.5.0",
    "prettier": "1.18.2",
    "rxjs-tslint": "0.1.7",
    "standard-version": "7.0.0",
    "tsickle": "0.37.0",
    "tslint-consistent-codestyle": "1.15.1",
    "tslint-defocus": "2.0.6",
    "tslint-eslint-rules": "5.4.0",
    "tslint-origin-ordered-imports-rule": "1.2.2",
    "tslint-rxjs-subject-restrictions-rule": "1.0.4",
    "karma": "4.3.0",
    "karma-jasmine": "2.0.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "cookie": "0.4.0"
  }
}
